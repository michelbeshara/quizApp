{
 "compilerOptions": {
   "allowSyntheticDefaultImports": true,
   "baseUrl": ".",
   "experimentalDecorators": true,
   "emitDecoratorMetadata": true,
   "lib": [
     "es2015",
     "es2016",
     "es2017",
     "dom"
   ],
   "module": "commonjs",
   "moduleResolution": "node",
   "outDir": "../dist/",
   "importHelpers": false,
   "noImplicitAny": true,
   "noImplicitThis": true,
   "noUnusedLocals": false,
   "paths": {
     "*": ["*"]
   },
   "suppressImplicitAnyIndexErrors": true,
   "skipLibCheck": true,
   "sourceMap": true,
   "strictNullChecks": true,
   "target": "es5",
   "typeRoots": [
     "../node_modules/@types/"
   ]
 },
 "exclude": [
   "../node_modules"
 ]
}
